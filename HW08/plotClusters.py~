import matplotlib.pyplot as plt
import numpy as np
#import pandas as pd

#df = pd.read_csv ('clusterdata.txt')
#print(df)


rows = []
with open('clusterdata.txt','r') as reader:
    for line in reader:
        # Remove the newline character
        line = line.strip()
        # Make a list of the strings found between commas
        fields = line.split(' ')
        rows.append(fields)

print(rows)
